{
  "name": "stokado",
  "type": "module",
  "version": "1.1.3",
  "description": "Stokado is a library that serves as a syntax sugar for storage, providing support for serialization, subscription-based listening, expiration setting, and one-time value.",
  "author": "KID-joker",
  "license": "MIT",
  "homepage": "https://github.com/KID-joker/stokado#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KID-joker/stokado.git"
  },
  "bugs": {
    "url": "https://github.com/KID-joker/stokado/issues"
  },
  "keywords": [
    "localStorage",
    "sessionStorage",
    "storage",
    "browser",
    "proxy",
    "serializer",
    "subscribe",
    "expires",
    "once",
    "disposable"
  ],
  "exports": {
    ".": {
      "types": "./dist/stokado.d.ts",
      "require": "./dist/stokado.cjs",
      "import": "./dist/stokado.mjs"
    }
  },
  "main": "dist/stokado.cjs",
  "module": "dist/stokado.mjs",
  "unpkg": "dist/stokado.min.js",
  "jsdelivr": "dist/stokado.min.js",
  "types": "dist/stokado.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.20.0"
  },
  "scripts": {
    "build": "rollup -c --environment BUILD:prod",
    "lint": "eslint . --fix",
    "test": "rollup -c --environment BUILD:test && npx playwright test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@playwright/test": "^1.32.3",
    "@rollup/plugin-html": "^1.0.1",
    "@types/node": "^18.7.10",
    "esbuild": "^0.17.17",
    "eslint": "^8.22.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-tsconfig-paths": "^1.5.0",
    "serve-then": "^1.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
